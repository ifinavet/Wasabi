{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "AZURE_BLOB_STRING_IN_HERE",
          "ContainerName": "BLOB_CONTAINER",
          "ContainerRootPath": ""
        }
      }
    },
    "CMS": {
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "Umbraco.Cms.Web.Common.PublishedModels",
        "FlagOutOfDateModels": true,
        "ModelsDirectory": "~/Models/Generated",
        "AcceptUnsafeModelsDirectory": false,
        "DebugLevel": 0
      },
      "Global": {
        "Id": "a8348306-ae24-4109-bca6-96f3f171d738",
        "UseHttps": true,
        "SanitizeTinyMce": true,
        "Smtp": {
          "From": "<fromEmail>",
          "Host": "<host>",
          "Port": 0
        } 
      },
      "Content": {
        "LoginLogoImage": "assets/logo_bla.png",
        "LoginBackgroundImage": "assets/DSCN4769.JPG",
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Security": {
        "AllowConcurrentLogins": false,
        "AllowPasswordReset": true
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "CONNECTION_STRING_IN_HERE"
  }
}